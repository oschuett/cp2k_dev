!!     unit_nr: the unit to write to (defaults to the standard out)
!!     error: variable to control error logging, stopping,... 
!!            see module cp_error_handling 
!!
!!   AUTHOR
!!     Fawzi Mohamed
!!
!!   MODIFICATION HISTORY
!!     4.2002 created [fawzi]
!!
!!*** **********************************************************************
  subroutine cp_sll_[nametype1]_write(sll,unit_nr,error)
    type(cp_sll_[nametype1]_type), pointer :: sll
    integer, intent(in) :: unit_nr
    type(cp_error_type), optional, intent(inout) :: error

    logical :: failure
    type(cp_sll_[nametype1]_type), pointer :: iterator
    character(len=*), parameter :: routineN='cp_sll_[nametype1]_write',&
         routineP=moduleN//':'//routineN
    [type1] :: el
