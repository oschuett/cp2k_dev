### Machine dependent ###

PERL	= perl
CC	= cc
CPP	= /usr/lib/cpp
FC	= f90 -freeform
FCfixed	= f90 -fixedform
LDR	= f90
AR	= ar -r

CDEFS	= -D__IRIX -D__FFTSG -D__FFTW

DEBUG	= -g -C
OPT	= -O3
BFLAGS	= -u $(CDEFS) -automatic
FFLAGS	= $(BFLAGS) $(OPT) -macro_expand
LFLAGS	= $(BFLAGS) $(OPT)

LIBS    = -L/usr/local/lib -lfftw -lcomplib.sgimath

OBJECTS_ARCHITECTURE	= machine_irix.o
