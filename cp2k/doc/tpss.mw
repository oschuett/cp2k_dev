icense.</Text-field><Text-field layout="Normal" style="Text">"Progress in science through free software" ;)</Text-field><Text-field layout="Normal" style="Text"/><Text-field layout="Normal" style="Text">Fawzi Mohamed</Text-field></Input></Group><Section collapsed="true"><Title><Text-field layout="Heading 1" style="Heading 1"> TPSS</Text-field></Title><Group><Input><Text-field layout="Normal" style="Text">Reference: Perdew,Tao, Staroverov, Scuseria, J.Chem Phys vol 120, p 6898 (2004)</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">restart;</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">sost:=eqs -&gt; subs(seq(eqs[nops(eqs)-i],i=1..(nops(eqs)-1)),rhs(eqs[nops(eqs)])):</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">unk:=eqs -&gt; indets(sost(eqs),symbol):</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">loc:=eqs -&gt; indets(eqs,symbol) minus unk(eqs):</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">e:='e': m:='m': h_bar:='h_bar': a_0:='a_0':myIF:='myIF':</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">e:=1: m:=1: h_bar:=1: a_0:=h_bar^2 /(m*e^2):</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">indice:=proc(el,l) local i,ii,elAtt,el_s;
 i:=-1; ii:=0; el_s:=convert(el,string);
 for elAtt in l do
  ii:=ii+1; 
  if evalb(el_s=convert(elAtt,string)) then
    i:=ii;
  end if;
 end do;
 i;
end proc:</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">indiceDef:=proc(el,l) local i,ii,elAtt;
 i:=-1; ii:=0;
 for elAtt in l do
  ii:=ii+1; 
  if evalb(el=lhs(elAtt)) then
    i:=ii;
  end if;
 end do;
 i;
end proc:</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">definizioni:= eqs -&gt; map(eq -&gt; if type(eq,equation) then lhs(eq); else 0; end if ,eqs):</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">sameNameSameDef:=proc(eqs1,eqs2) local commonDef,res,d;
 commonDef:=convert(definizioni(eqs1),set)intersect convert(definizioni(eqs2),set);
 res:=true;
 for d in commonDef do
  if not evalb(subs(eqs1,d)=subs(eqs2,d)) then;
   print("def different for "||d);
   res:=false;
  end if;
 end do;
 res;
end proc:</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input"># check same name -&gt; same def apart from eqs at the indexes returned by the function eqs_to_rm
checkCompatible:=proc (eqss,eqs_to_rm) local i,j,im_indx,eqd1,eqd2,res,ii,attComp;
  res:=true;
  for i from 1 to nops(eqss)-1 do
    im_indx:=eqs_to_rm(eqss[i]);
    #print("removed",map(lhs,[eqss[i][im_indx[ii]]$ii=1..nops(im_indx)]));
    eqd1:=subsop('im_indx[ii]=NULL'$ii=1..nops(im_indx),eqss[i]):
    for j from i+1 to nops(eqss) do
      #print("doing (",i,j,")");
      im_indx:=eqs_to_rm(eqss[j]);
      #print("removed",map(lhs,[eqss[j][im_indx[ii]]$ii=1..nops(im_indx)]));
      eqd2:=subsop('im_indx[ii]=NULL'$ii=1..nops(im_indx),eqss[j]):
      attComp:=sameNameSameDef(eqd1,eqd2);
      res:=attComp and res;
      if not attComp then
        print("incompatibility between",i,j);
      end if;
    end do;
  end do;
  res;
end proc: </Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">getDef:=proc(symb,eqs) local eq;
 for eq in eqs do
  if(lhs(eq)=symb) then
   return eq;
  end if;
 end do;
 0;
end proc:</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">eqUses:=(eq1,eq2)-&gt;evalb(lhs(eq2) in indets(rhs(eq1),symbol)):</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">enforceDependencies:=proc(eqs) local dep,eq1,eq2,i,j,ii,eqns;
 dep:=true;
 eqns:=eqs;
 ii:=0;
 i:=1;
 while (i&lt;=(nops(eqs)-1) and ii&lt;10000) do
  dep:=false;
  j:=i+1;
  while (j&lt;=nops(eqs) and ii&lt;10000) do
   if eqUses(eqns[i],eqns[j]) then
    ii:=ii+1;
    eqns:=subsop(i=NULL,j=(eqns[j],eqns[i]),eqns);
    dep:=true;
   else
    j:=j+1;
   end if;
  end do;
  if not dep then i:=i+1; end if;
 end do;
 eqns;
end proc:     </Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">combineDefs:=proc(ord) local def,defs,allDefs;
 allDefs:=[]:
 for defs in ord do
  for def in defs do
   if not def in allDefs then
    allDefs:=[op(allDefs),def];
   end if;
  end do;
 end do;
 allDefs;
end proc:</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">combineEqs:=proc(allDefs,eqss,ord) local def,eqs,eqsDeriv,found,d,i,ii;
 eqsDeriv:=[];
 for def in allDefs do
  found:=false;
  for ii from 1 to nops(eqss) do
   d:=ord[ii];
   i:=indice(def,d);
   if i&gt;0 then
    eqs:=eqss[ii];
    if not (lhs(eqs[i])=def) then print("errore eq",def); end if;
    eqsDeriv:=[op(eqsDeriv),eqs[i]];
    found:=true;
    break;
   end if;
  end do;
  if not found then print("error unknown def",def); end if;
 end do;
 eqsDeriv;
end proc:</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">sostConst:=proc(eqs) local sAtt,sToDo,result;
 sToDo:=[];
 result:=[];
 for sAtt in eqs do
  sAtt:=subs(op(sToDo),sAtt);
  if type(rhs(sAtt),numeric) then sToDo:=[op(sToDo),sAtt]; end if;
  if rhs(sAtt)&lt;&gt;0 then result:=[op(result),sAtt]; end if;
 end do;
 result;
end proc:</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">calcDerivs:=proc(eqs,arg_names) local cs,r,d,eq,eq2,eq3,i;
 cs:=CompSeq(locals=loc(eqs),globals=convert(unk(eqs),set)minus convert(arg_names,set),
  params=arg_names,eqs);
 r:=convert(cs,procedure);
 d:=[seq(D[i](r),i=1..nops(arg_names))];
 eq:=map(f-&gt;op(-1,convert(f,CompSeq)),d);
 # ensure that the variables are bound in the global namespace
 eq2:=map(f-&gt;evalindets(f,symbol,g-&gt;convert(convert(g,string),symbol)),eq);
 eq3:=[seq(subs(result=deriv_||(arg_names[i]),eq2[i]),i=1..nops(arg_names))];
end proc:</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">with(CodeGeneration);</Text-field></Input><Output><Text-field layout="Warning" style="Warning">Warning, the protected name Matlab has been redefined and unprotected</Text-field></Output><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiM3LEkiQ0c2IkkoRm9ydHJhbkdGJUkxSW50ZXJtZWRpYXRlQ29kZUdGJUklSmF2YUdGJUkzTGFuZ3VhZ2VEZWZpbml0aW9uR0YlSSdNYXRsYWJHNiRJKnByb3RlY3RlZEdGLEkoX3N5c2xpYkdGJUkmTmFtZXNHRiVJJVNhdmVHRiVJKlRyYW5zbGF0ZUdGJUksVmlzdWFsQmFzaWNHRiU=</Equation></Text-field></Output></Group><Section><Title><Text-field layout="Heading 2" style="Heading 2">exchange</Text-field></Title><Group><Input><Text-field layout="Normal" style="Text">exhange energy (LDA)</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">eqx1:=ex_lda=rho*ex_unif*Fx;</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiM+SSVlcXgxRzYiL0knZXhfbGRhR0YlKihJJHJob0dGJSIiIkkoZXhfdW5pZkdGJUYqSSNGeEdGJUYq</Equation></Text-field></Output></Group><Group><Input><Text-field layout="Normal" style="Text">Uniform gas exchange:</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">eqx2:=ex_unif=-3/(4*Pi)*(3*Pi^2*rho)^(1/3);</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiM+SSVlcXgyRzYiL0koZXhfdW5pZkdGJSwkKihJI1BpR0kqcHJvdGVjdGVkR0YrISIiIiIkIyIiIkYtKiZGKiIiI0kkcmhvR0YlRi9GLiMhIiQiIiU=</Equation></Text-field></Output></Group><Group><Input><Text-field layout="Normal" style="Text">The enhancement factor Fx is function of just p an z;</Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">eqx3:=p=norm_drho^2/(4*(3*Pi^2)^(2/3)*rho^(8/3));
