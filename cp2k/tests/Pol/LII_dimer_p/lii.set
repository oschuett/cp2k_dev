&SETUP
  molecule
    name     iodine  
    nummol   1      
    nhcopt   global  
  end molecule
  molecule
    name     lithium
    nummol    1    
    nhcopt   global  
  end molecule
  cell
   parameter   15.0   0.0    0.0  &
                0.0   15.0    0.0  &
                0.0   0.0    15.0
  end
&END

&MOLECULES
  iodine  
    numat       1
    atom        i 1  126.9040   0.0   
    pol_set    i  I_DRHO  30   1E-62
    rho0_set   i  I_RHO0 30    1E-40   
  end iodine
  lithium  
    numat      1
    atom        li 1  6.9390  0.0  
    pol_set    li  LI_DRHO  30   1E-60   
    rho0_set   li  LI_RHO0  30  1E-30   
  end lithium
&END

&FORCE_FIELD
  nonbonded
 williams       i i      2.1955602E+6    2.0666998      0.0   6.5
 williams       i li         3.8029557E+6    2.0905671         0.0      6.5
 williams       li li   5.321E+3     5.46        0.0         6.5
  end nonbonded
&END

&EMPIRICAL_PARAMETERS
i
nset 2
empar 1  0  -.269296   -.127996170E+01
empar 2  1  0.0   -.30668306E+01
end i
li
nset 1
empar 1 0   -1.212   .2745056E+02  
end li
&END
