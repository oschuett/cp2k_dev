
&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      parmfile sample_pot/aala.pot
      parmtype CHM
    &END FORCEFIELD
    &POISSON_MM
      &EWALD
        EWALD_TYPE ewald
        ALPHA .07
        GMAX 29
      &END EWALD
    &END POISSON_MM
  &END MM
  &SUBSYS
    &CELL
      ABC 50.0 50.0 50.0
      UNIT ANGSTROM
    &END CELL
    &TOPOLOGY
      CHARGE_BETA
      COORD_FILE_NAME sample_pdb/aala.pdb
      COORDINATE PDB
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  FFTLIB FFTSG
  PRINT_LEVEL LOW

  PROJECT aala
  RUN_TYPE debug
&END GLOBAL
