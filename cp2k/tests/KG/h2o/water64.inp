&CP2K
  PROJECT    h2o
  PROGRAM    kg
  IOLEVEL    4
  RUN_TYPE   MD     
  FFTLIB     FFTSG
&END


&DFT
X-FUNCTIONAL  none
C-FUNCTIONAL  pade 
KE-FUNCTIONAL llp
density_cutoff   1.0E-10
gradient_cutoff   1.0E-10
&END 

&KG
 cutoff 140.0
 eps_core_charge 1.0E-96 
 eps_ppl 1.0E-96
 eps_pgf_orb 1.0E-16
 eps_gvg_rspace 1.0E-16
 eps_rho 1.0E-16
 method  KG_NOPOL
 ngrid_level 1
&END

&CONSTRAINTS
    g3x3 add 1 1 2 3 1.872719336 1.872719336 2.981376694
&END CONSTRAINTS

&PRINT low
BASIS_SET
CORE_CHARGE_RADII
RADII
POTENTIALS
GRID_INFORMATION
CUBE
&END

&KIND O 
BASIS_SET KG-FEDPD-BARKER
POTENTIAL ELP
&END

&KIND H
BASIS_SET KG-FEDPD-BARKER
POTENTIAL ELP
&END


&MD
  ensemble:      nvt                      (nve,nvt,npt_i,npt_f)
  steps:         20000 
  timestep:    1.0   fs                 time step [fs]
  temperature:  300.0000                  temperature [K]
  restart:      posvel                    start type (pos,posvel)
output_format: atomic
  constraints:  on
  nose_parameter   &
    length:    1        &
    Yoshida:   3        &
    timecon:   1000.  fs   &
    mts:       2
  barostat_parameter:   &
    pressure: 0.        &
    timecon: 1000.
  coef_ensemble:    nvt
  coef_temperature  2.0                  coef temperature
  coef_storage:     global
  coef_mass:        250
  coef_restart: pos
  coef_nose_parameter   &
   option:    global &
   length:    1   &
   Yoshida:   3   &
   mts:       2     &
   timecon:   100 fs
  print  files          10 
  print  screen        10   
&END

&CELL
 UNIT         ANGSTROM
 ABC          12.386 12.386 12.386          
 SUBCELLS  0.
&END

