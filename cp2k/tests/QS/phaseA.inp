&CP2K
 PROGRAM      Quickstep
 IOLEVEL      10
 FFTLIB       FFTESSL
## RUN_TYPE     MD 
&END

&TOPOL
 connectivity generate
 bondparm vdw 0.0
&END

&MD
  ensemble:     nve                    (nve,nvt,npt_i,npt_f)
  steps:        2 
  timestep:     0.1                       time step [fs]
  temperature:  300.0                      temperature [K]
  restart:      init                      start type (pos,posvel)
  constraints:  off
  shake:        1.e-10
  verlet_skin:   1
  nose_parameter   &
    length:    3        &
    Yoshida:   3        &
    timecon:   1000.    &
    mts:       2
  barostat_parameter:   &
    pressure: 0.        &
    timecon: 1000.
  print  files          10
  print  screen         10
&END

&DFT
 FUNCTIONAL  BLYP 
&END

&OTSCF
 MINIMIZER DIIS 7
&END

&QS
 NGRID        1
 CUTOFF       300
 EPS_DEFAULT  1.0E-12
&END

&SCF
 GUESS ATOMIC
 MIXING       0.05
 EPS_SCF      1.0E-6
# SMEAR        0.02
# OT
 MAX_SCF      2
&END

&PRINT low
# forces
# NO_COORDINATES
&END

&KIND H
 BASIS_SET    DZV-GTH-BLYP
 POTENTIAL    GTH-BLYP
 MASS 1.00794
&END


&KIND O
 BASIS_SET    DZVP-GTH-BLYP
 POTENTIAL    GTH-BLYP
&END


