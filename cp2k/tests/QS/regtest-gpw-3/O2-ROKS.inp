&GLOBAL
  FFTLIB FFTSG
  PROGRAM Quickstep
  PROJECT O2
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL

&FORCE_EVAL
  &DFT
    ROKS
    MULTIP 3
    &MGRID
      CUTOFF 200
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-8
    &END QS
    &SCF
      MIXING 0.2
      SCF_GUESS atomic
    &END SCF
    &XC
      &XC_FUNCTIONAL Pade
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 4.0 4.0 6.0
      UNIT ANGSTROM
    &END CELL
    &KIND O
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q6
    &END KIND
    &COORD
      O  0.000000  0.000000  0.608000
      O  0.000000  0.000000 -0.608000
    &END COORD
  &END SUBSYS
&END FORCE_EVAL
