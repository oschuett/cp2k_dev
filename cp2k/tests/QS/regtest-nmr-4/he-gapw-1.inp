&GLOBAL
  PROJECT he-gapw-1
  PRINT_LEVEL LOW
  RUN_TYPE LINEAR_RESPONSE
&END GLOBAL

&FORCE_EVAL
  &DFT
    BASIS_SET_FILE_NAME ../EMSL_BASIS_SETS
    POTENTIAL_FILE_NAME ../POTENTIAL
    &MGRID
      CUTOFF 150
    &END MGRID
    &QS
      METHOD GAPW
    &END QS
    &SCF
      MAX_SCF 5
      SCF_GUESS ATOMIC
    &END SCF
    &XC
      &XC_FUNCTIONAL BLYP
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &PROPERTIES
    &LINRES
       MAX_SCF 10
       LOCALIZE_PSI0
       OPT_METHOD CG
       LINESEARCH 2PNT
       PRECONDITIONER FULL_ALL
       &NMR
          FULL
          &PRINT
            &CHI_TENSOR
            &END CHI_TENSOR
          &END PRINT
       &END
    &END
  &END
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
      UNIT ANGSTROM
    &END CELL
    &COORD
 HE   0.000000    0.000000   0.000000
    &END COORD
    &KIND HE
      BASIS_SET 6-31G**
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL


